#

#ex:NodeLabelMapping a rdfs:Class ;

#ex:LiteralBasedNodeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:NodeLabelMapping .
#
#ex:IRIBasedNodeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:NodeLabelMapping .
#

#ex:EdgeLabelMapping a rdfs:Class ;
#
#ex:IRIBasedEdgeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:EdgeLabelMapping .

#ex:RegexBasedEdgeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:EdgeLabelMapping .

#ex:SingleEdgeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:EdgeLabelMapping .

#ex:CombinedEdgeLabelMapping a rdfs:Class ;
#   rdfs:subClassOf ex:EdgeLabelMapping .

#ex:PropertyNameMapping a rdfs:Class ;
#
#ex:IRIBasedPropertyNameMapping a rdfs:Class ;
#   rdfs:subClassOf ex:PropertyMapping .



