PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd:    <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX lr:     <http://www.example.org/se/liu/ida/hefquin/lpg2rdf#>
PREFIX ex:     <http://example.org/>

# Classes
#ex:LPGtoRDFConfiguration a rdfs:Class ;

#ex:NodeMapping a rdfs:Class ;

#ex:BNodeBasedNodeMapping a rdfs:Class ;
#   rdfs:subClassOf ex:NodeMapping .
#
#ex:IRIBasedNodeMapping a rdfs:Class ;
